{
  "name": "@rline/fs",
  "version": "8.1.1",
  "description": "Read, write, manage files and directories",
  "license": "MIT",
  "author": {
    "email": "robert.brightline@gmail.com",
    "name": "Robert Brightline",
    "url": "https://rbrightline.github.io"
  },
  "homepage": "https://rbrightline.github.io/rline/fs",
  "repository": {
    "url": "https://github.com/rbrightline/common/tree/main/libs/fs",
    "type": "github"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "keywords": [
    "io",
    "read",
    "write",
    "file",
    "directory"
  ],
  "dependencies": {
    "@rline/type": "workspace:*",
    "@rline/utils": "workspace:*",
    "@swc/helpers": "~0.5.11"
  },
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "nx": {
    "sourceRoot": "libs/fs/src",
    "projectType": "library",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "libs/fs/dist",
          "main": "libs/fs/src/index.ts",
          "tsConfig": "libs/fs/tsconfig.lib.json",
          "skipTypeCheck": true,
          "stripLeadingPaths": true
        }
      }
    },
    "name": "fs"
  }
}