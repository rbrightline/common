{
  "name": "@rline/logger",
  "version": "9.4.0",
  "description": " library",
  "author": {
    "email": "robert.brightline@gmail.com",
    "name": "Robert Brightline",
    "url": "https://rbrightline.github.io"
  },
  "homepage": "https://rbrightline.github.io/common/logger",
  "repository": {
    "url": "https://github.com/rbrightline/common/tree/main/libs/logger",
    "type": "github"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "keywords": [
    "rline",
    "library"
  ],
  "scripts": {
    "workspace:remove": "pnpm remove @rline/color",
    "workspace:add": "pnpm add --workspace @rline/color",
    "workspace": "pnpm workspace:remove && pnpm workspace:add"
  },
  "dependencies": {
    "@rline/color": "9.4.0",
    "@rline/type": "9.4.0",
    "@swc/helpers": "~0.5.11"
  },
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "nx": {
    "sourceRoot": "libs/logger/src",
    "projectType": "library",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "libs/logger/dist",
          "main": "libs/logger/src/index.ts",
          "tsConfig": "libs/logger/tsconfig.lib.json",
          "skipTypeCheck": false,
          "stripLeadingPaths": true
        }
      }
    },
    "name": "logger"
  }
}
