{
  "name": "@rline/rline",
  "version": "0.0.1",
  "author": {
    "email": "robert.brightline@gmail.com",
    "name": "Robert Brightline",
    "url": "https://rbrightline.github.io"
  },
  "homepage": "https://rbrightline.github.io/rline/rline",
  "repository": {
    "url": "https://github.com/rbrightline/common/tree/main/libs/names",
    "type": "github"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "keywords": [
    "generator",
    "api",
    "client",
    "library",
    "angular",
    "nestjs",
    "nx"
  ],
  "description": "Project generators",
  "dependencies": {
    "@nx/devkit": "20.3.2",
    "@swc/helpers": "~0.5.11"
  },
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "!**/*.tsbuildinfo",
    "generators.json"
  ],
  "module": "./dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "sourceRoot": "libs/rline/src",
    "projectType": "library",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "libs/rline/dist",
          "main": "libs/rline/src/index.ts",
          "tsConfig": "libs/rline/tsconfig.lib.json",
          "skipTypeCheck": true,
          "stripLeadingPaths": true,
          "assets": [
            {
              "input": "./libs/rline/src",
              "glob": "**/!(*.ts)",
              "output": "."
            },
            {
              "input": "./libs/rline/src",
              "glob": "**/*.d.ts",
              "output": "."
            }
          ]
        }
      }
    },
    "name": "rline"
  },
  "generators": "./generators.json"
}