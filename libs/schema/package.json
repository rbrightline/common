{
  "name": "@rline/schema",
  "version": "1.0.1",
  "description": "Project description",
  "author": {
    "email": "robert.brightline@gmail.com",
    "name": "Robert Brightline",
    "url": "https://rbrightline.github.io"
  },
  "homepage": "https://rbrightline.github.io/common/schema",
  "repository": {
    "url": "https://github.com/rbrightline/common/tree/main/libs/schema",
    "type": "github"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "keywords": [
    "rline",
    "library"
  ],
  "dependencies": {
    "@swc/helpers": "~0.5.11"
  },
  "files": [
    "*.schema.json"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "nx": {
    "sourceRoot": "libs/schema/src",
    "projectType": "library",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "libs/schema/dist",
          "main": "libs/schema/src/index.ts",
          "tsConfig": "libs/schema/tsconfig.lib.json",
          "skipTypeCheck": true,
          "stripLeadingPaths": true
        }
      }
    },
    "name": "schema"
  }
}