{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "IntegerPropertyOptions",
    "type": "object",
    "allOf": [
        {
            "$ref": "./common-number.schema.json"
        },
        {
            "properties": {
                "type": {
                    "const": "integer"
                },
                "default": {
                    "type": "integer"
                },
                "isIn": {
                    "type": "array",
                    "items": {
                        "oneOf": [
                            {
                                "type": "integer"
                            }
                        ]
                    }
                },
                "isNotIn": {
                    "type": "array",
                    "items": {
                        "oneOf": [
                            {
                                "type": "integer"
                            }
                        ]
                    }
                }
            },
            "required": [
                "type"
            ]
        }
    ]
}