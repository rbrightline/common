{
  "name": "@rline/utils",
  "version": "9.0.2",
  "description": "Common utilities",
  "license": "MIT",
  "author": {
    "email": "robert.brightline@gmail.com",
    "name": "Robert Brightline",
    "url": "https://rbrightline.github.io"
  },
  "homepage": "https://rbrightline.github.io/common/utils",
  "repository": {
    "url": "https://github.com/rbrightline/common/tree/main/libs/utils",
    "type": "github"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "keywords": [
    "utils",
    "helpers",
    "common"
  ],
  "scripts": {
    "workspace:remove": "pnpm remove  @rline/type",
    "workspace:add": "pnpm add --workspace  @rline/type",
    "workspace": "pnpm workspace:remove && pnpm workspace:add"
  },
  "dependencies": {
    "@rline/type": "9.0.1",
    "@swc/helpers": "~0.5.11"
  },
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "nx": {
    "sourceRoot": "libs/utils/src",
    "projectType": "library",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "libs/utils/dist",
          "main": "libs/utils/src/index.ts",
          "tsConfig": "libs/utils/tsconfig.lib.json",
          "skipTypeCheck": false,
          "stripLeadingPaths": true
        }
      }
    },
    "name": "utils"
  }
}